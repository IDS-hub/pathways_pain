<div class="container">
  <% flash.each do |key, value| %>
    <div class="alert alert-<%= key %>"><%= value %></div>
  <% end %>
</div>

<div>
	<%= form_tag sign_in_admin_index_path do %>
		<%= label_tag :login %>
		<%= text_field_tag :login %>

		<%= label_tag :password %>
		<%= password_field_tag :password %>

		<%= submit_tag :login %>
	<%end%>
</div>
